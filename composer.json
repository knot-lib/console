{
  "name": "knot-lib/console",
  "description": "Console utilities for kNot Framework.",
  "keywords": ["PHP","PHP7","kNot Framework","console"],
  "homepage": "https://github.com/knot-lib/console",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@knot-framework.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "stk2k/argparser": "~0.1",
    "knot-lib/module": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3.0",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "KnotLib\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KnotLib\\Console\\Test\\": "test/include/",
      "KnotLib\\Console\\Demo\\": "demo/include/"
    }
  }
}